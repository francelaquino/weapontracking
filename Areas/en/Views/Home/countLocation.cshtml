@model List<FMS.Models.mdlTrack>
@{
    Layout = null;
}

<ul class="feeds">
    @{
        foreach (var records in Model)
        {
            <li>
                <div class="col1">
                    <div class="cont">
                        <div class="cont-col1">
                            <div class="label label-sm label-info">
                                @records.COUNT
                            </div>
                        </div>
                        <div class="cont-col2">
                            <div class="desc">
                                @records.LOCATION

                            </div>
                        </div>
                    </div>
                </div>

            </li>
        }
    }
</ul>



